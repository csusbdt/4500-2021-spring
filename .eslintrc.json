{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "script"
    },
    "plugins": [
        "eslint-plugin-html"
    ],
    "rules": {
        "no-eq-null" : "error",
        "eqeqeq" : "error"
    }
}
