{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": ["eslint:recommended", "plugin:compat/recommended"],
	"parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "plugins": [
        "eslint-plugin-html"
    ],
    "rules": {
        "no-eq-null" : "error",
        "eqeqeq" : "error"
    }
}
